{
  "name": "portal-vue",
  "version": "3.0.0-beta.0",
  "license": "MIT",
  "repository": "https://github.com/LinusBorg/portal-vue",
  "author": {
    "name": "Thorsten LÃ¼nborg",
    "url": "https://github.com/LinusBorg/"
  },
  "private": false,
  "main": "dist/portal-vue.umd.js",
  "jsdelivr": "dist/portal-vue.umd.js",
  "unpkg": "dist/portal-vue.umd.js",
  "module": "dist/portal-vue.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./": "./",
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/portal-vue.es.js",
      "require": "./dist/portal-vue.umd.js"
    },
    "./dev": {
      "import": "./dist/portal-vue.es.dev.js",
      "require": "./dist/portal-vue.umd.dev.js"
    }
  },
  "files": [
    "./dist",
    "package.json"
  ],
  "scripts": {
    "app:dev": "cd example && vite",
    "lib:build": "vite build && vite build -c vite.config.dev.ts && rollup -c scripts/rollup.types.config.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "lint": "eslint --fix './*.{js,ts}' 'src/**/*.{ts,tsx,vue}' 'example/**/*.{ts,tsx,vue}'",
    "test": "yarn jest --runInBand"
  },
  "engines": {
    "node": ">=14"
  },
  "peerDependencies": {
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@vitejs/plugin-vue": "^1.1.0",
    "@vue/babel-preset-app": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "autoprefixer": "^10.2.4",
    "babel-jest": "^26.6.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "jest": "^26.6.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.4.2",
    "nanoid": "^3.1.13",
    "prettier-eslint": "^11.0.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass": "^1.28.0",
    "sass-loader": "^10.0.3",
    "ts-jest": "26.4.1",
    "typescript": "^4.1.0",
    "vite": "^2.0.0-beta.36",
    "vitepress": "^0.12.0",
    "vue": "^3.0.4",
    "vue-jest": "^5.0.0-alpha.5",
    "vue-router": "^4.0.0-0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module-compact",
      "options": {}
    }
  }
}
